  "scripts": {
    "start:webpack": "NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0",
    "start": "yarn start:webpack",
    "build": "rm -rf ./release && yarn build:wp",
    "build:imported": "imported-components src src/async-requires.js",
    "build:wp": "yarn build:imported && NODE_ENV=production webpack --config webpack.config.js -p",
    "build:dev": "build-imported && NODE_ENV=development webpack --config webpack.config.all.js",
    "release": "yarn build && cp ./public/now.json ./release",
    "storybook": "NODE_ENV=development start-storybook -p 9009 -s public --ci",
    "storybook:build": "NODE_ENV=development build-storybook -c .storybook -o build/storybook",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,json}'",
    "prettier-watch": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
    "lint": "tslint \"src/**/*.{ts,tsx}\" --fix",
    "now:deploy": "yarn release && yarn storybook:build && rm -Rf ./release/storybook && cp -R ./build/storybook ./release && yarn now:release",
    "now:release": "cd release && now --public --prod",
    "stat:build": "webpack --profile --json > stats.json",
    "stat:build:prod": "NODE_ENV=production webpack -p --profile --json > stats.json",
    "stat": "webpack-bundle-analyzer stats.json"
  },
  "reactSnap": {
    "source": "release",
    "skipThirdPartyRequests": true
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
